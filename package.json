{
  "name": "english",
  "version": "1.0.0",
  "description": "english test application",
  "main": "server.ts",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "nodemon src/server.ts",
    "jasmine": "set ENV=test &&jasmine --config=jasmine.json",
    "test": "npm run build && set ENV=test&& npm run jasmine",
    "migrate": "db-migrate up",
    "demigrate": "db-migrate reset",
    "migrate-test": "db-migrate --env test up",
    "demigrate-test": "db-migrate --env test reset",
    "lint": "eslint . --ext .ts",
    "format": "prettier --config .prettierrc.json src/**/*.ts --write",
    "format:check": "prettier --config .prettierrc.json src/**/*.ts --check"
  },
  "author": "mohamed elewasy",
  "license": "ISC",
  "dependencies": {
    "@types/pg": "^8.6.6",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-validator": "^6.15.0",
    "jsonwebtoken": "^9.0.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.1",
    "pg": "^8.9.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.29.0",
    "uuidv4": "^6.2.13",
    "validator": "^13.9.0"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/express-validator": "^3.0.0",
    "@types/jasmine": "^4.3.1",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.14.1",
    "@types/nodemailer": "^6.4.7",
    "@types/sequelize": "^4.28.14",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.7.12",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine": "^4.5.0",
    "jasmine-spec-reporter": "^7.0.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.4",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
